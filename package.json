{
  "name": "@sibiaoke/utils",
  "version": "0.1.3",
  "description": "A frontend utils library",
  "type": "module",
  "main": "dist/sibiaoke-utils.umd.js",
  "module": "dist/sibiaoke-utils.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/sibiaoke-utils.es.js",
      "require": "./dist/sibiaoke-utils.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sibiaoke/frontend-utils.git"
  },
  "keywords": [
    "frontend",
    "utils"
  ],
  "author": "vickyhoo <vickyhoo.dev@gmail.com>",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.0.2",
    "happy-dom": "^14.12.3",
    "msw": "^2.3.1",
    "prettier": "3.3.3",
    "release-it": "^17.6.0",
    "release-it-pnpm": "^4.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^3.9.1",
    "vitepress": "^1.3.0",
    "vitest": "^2.0.2"
  },
  "dependencies": {
    "ky": "^1.4.0"
  }
}
